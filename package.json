{
  "name": "hunter-ai-graphrag",
  "version": "0.1.0",
  "description": "Hunter AI Enhanced GraphRAG System - Intelligent hyperlocal journalism platform",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:reset": "npm run db:migrate && npm run db:seed",
    "deploy:staging": "railway up --environment staging",
    "deploy:production": "railway up --environment production",
    "workflows:export": "node scripts/export-workflows.js",
    "workflows:import": "node scripts/import-workflows.js"
  },
  "keywords": [
    "journalism",
    "graphrag",
    "ai",
    "hyperlocal",
    "community",
    "n8n",
    "supabase",
    "knowledge-graph"
  ],
  "author": "Hunter AI Team",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}